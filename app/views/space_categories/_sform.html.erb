<%= form_for([@basic, @basic.occupant_categories.build]) do |f| %>
  <form>
  	<div class="page-header space_hearder">
			<h4>Name</h4>
		</div>
		<div class="row my_row">
			<div class="col-lg-3 col-md-3 col-sm-6 col-xs-6 my_table">	  
			  <div class="form-group">		    	
				  <%= f.text_field :name, class: "form-control" %>
			  </div>
		  </div>		  	  		  
		</div>
				<div class="page-header space_hearder">
					<h4>Event</h4>
				</div>			
				<%= render 'tb1' %>
			
				<div class="page-header space_hearder">
					<h4>Space occupancy</h4>
				</div>			
				<%= render 'tb2' %>
		<div class="row my_row">
			<div class="col-lg-10 col-md-10 col-sm-10 col-xs-10"></div>
			<div class="col-lg-1 col-md-1 col-sm-1 col-xs-1">
				<%= link_to 'Cancel', edit_basic_path(@basic), class: "btn btn-large btn-default"%>
			</div>
			<div class="col-lg-1 col-md-1 col-sm-1 col-xs-1">
				<%= f.submit "Add" , class: "btn btn-large btn-primary"%>
			</div>			
		</div>
	</form>
<% end %>
