<div class="container">
	<ul class="nav nav-tabs my_nav">
	  <li role="presentation"><%= link_to 'Edit Building', edit_basic_path(@basic) %></li>
	  <li role="presentation"><%= link_to 'Edit Space', basic_space_categories_path(@basic) %></li>
	  <li role="presentation" class="active"><%= link_to 'Edit Occupant', basic_occupant_categories_path(@basic)%></li>
	</ul>
	<% @basic.occupant_categories.each_with_index do |occupant_category, index| %>
		<%= form_for occupant_category, :url => basic_occupant_category_path(@basic,occupant_category), :html => {:method => :patch} do |f| %>
			<div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
			  <div class="panel panel-default">
			    <div class="panel-heading" role="tab" id="heading<%= index%>">
			      <h4 class="panel-title">
			        <a role="button" data-toggle="collapse" data-parent="#accordion" href="#collapse<%= index%>" aria-expanded="true" aria-controls="collapse<%= index%>">
			          <%= occupant_category.name %>
			        </a>
			      </h4>
			    </div>
				  <div id="collapse<%= index%>" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="heading<%= index%>">
			      <div class="panel-body">
			      	<div class="row my_row">
								<div class="col-lg-6 col-md-12 col-sm-12 col-xs-12 my_table">
									<div class="page-header space_hearder">
										<h4>Event</h4>
									</div>			
									<div class = "media">
										<div class = "media-body">
											<table class = "table table-hover">
											  <thead>
											    <tr>
											    	<th>Event</th>
											      <th>Typical Time</th>
											      <th>Start Time</th>
											      <th>End Time</th>
											    </tr>
											  </thead>
											  <tbody>
											  	<tr>
											  		<td>Arrival</td>
											      <td>
													  	<div class="form-group my_td">		    	
														  	<%= f.time_field :ATypical, class: "form-control" %>
													    </div>
													  </td>		  		  
													  <td>
													  	<div class="form-group my_td">		    	
														  	<%= f.time_field :AStart, class: "form-control"%>
													    </div>
													  </td>
													  <td>
													  	<div class="form-group my_td">		    	
														  	<%= f.time_field :AEnd, class: "form-control"%>
													    </div>
													  </td>
											    </tr>
											    <tr>
											  		<td>Work morning</td>
											      <td>
													  	<div class="form-group my_td">		    	
														  	<%= f.time_field :WMTypical, class: "form-control" %>
													    </div>
													  </td>		  		  
													  <td>
													  	<div class="form-group my_td">		    	
														  	<%= f.time_field :WMStart, class: "form-control"%>
													    </div>
													  </td>
													  <td>
													  	<div class="form-group my_td">		    	
														  	<%= f.time_field :WMEnd, class: "form-control"%>
													    </div>
													  </td>
											    </tr>
											    <tr>
											  		<td>Go to lunch</td>
											      <td>
													  	<div class="form-group my_td">		    	
														  	<%= f.time_field :GTLTypical, class: "form-control" %>
													    </div>
													  </td>		  		  
													  <td>
													  	<div class="form-group my_td">		    	
														  	<%= f.time_field :GTLStart, class: "form-control"%>
													    </div>
													  </td>
													  <td>
													  	<div class="form-group my_td">		    	
														  	<%= f.time_field :GTLEnd, class: "form-control"%>
													    </div>
													  </td>
											    </tr>
											    <tr>
											  		<td>Back from lunch</td>
											      <td>
													  	<div class="form-group my_td">		    	
														  	<%= f.time_field :BFLTypical, class: "form-control" %>
													    </div>
													  </td>		  		  
													  <td>
													  	<div class="form-group my_td">		    	
														  	<%= f.time_field :BFLStart, class: "form-control"%>
													    </div>
													  </td>
													  <td>
													  	<div class="form-group my_td">		    	
														  	<%= f.time_field :BFLEnd, class: "form-control"%>
													    </div>
													  </td>
											    </tr>
											    <tr>
											  		<td>Work afternoon</td>
											      <td>
													  	<div class="form-group my_td">		    	
														  	<%= f.time_field :WATypical, class: "form-control" %>
													    </div>
													  </td>		  		  
													  <td>
													  	<div class="form-group my_td">		    	
														  	<%= f.time_field :WAStart, class: "form-control"%>
													    </div>
													  </td>
													  <td>
													  	<div class="form-group my_td">		    	
														  	<%= f.time_field :WAEnd, class: "form-control"%>
													    </div>
													  </td>
											    </tr>
											    <tr>
											  		<td>Departure</td>
											      <td>
													  	<div class="form-group my_td">		    	
														  	<%= f.time_field :DTypical, class: "form-control" %>
													    </div>
													  </td>		  		  
													  <td>
													  	<div class="form-group my_td">		    	
														  	<%= f.time_field :DStart, class: "form-control"%>
													    </div>
													  </td>
													  <td>
													  	<div class="form-group my_td">		    	
														  	<%= f.time_field :DEnd, class: "form-control"%>
													    </div>
													  </td>
											    </tr>
											  </tbody>
											</table>
										</div>
									</div>
								</div>
								
								<div class="col-lg-6 col-md-12 col-sm-12 col-xs-12 my_table">
									<div class="page-header space_hearder">
										<h4>Space occupancy</h4>
									</div>			
									<div class = "media">
										<div class = "media-body">
											<table class = "table table-hover">
											  <thead>
											    <tr>
											    	<th>Location</th>
											      <th>Present percent</th>
											      <th>Duration</th>
											    </tr>
											  </thead>
											  <tbody>
											  	<tr>
											  		<td>Own office</td>
											      <td>
													  	<div class="form-group my_td">		    	
														  	<%= f.number_field :ownPercent, class: "form-control" %>
													    </div>
													  </td>		  		  
													  <td>
													  	<div class="form-group my_td">		    	
														  	<%= f.time_field :ownDuration, class: "form-control"%>
													    </div>
													  </td>
											    </tr>
											    <tr>
											  		<td>Other office</td>
											      <td>
													  	<div class="form-group my_td">		    	
														  	<%= f.number_field :otherPercent, class: "form-control" %>
													    </div>
													  </td>		  		  
													  <td>
													  	<div class="form-group my_td">		    	
														  	<%= f.time_field :otherDuration, class: "form-control"%>
													    </div>
													  </td>
											    </tr>
											    <tr>
											  		<td>Meeting room</td>
											      <td>
													  	<div class="form-group my_td">		    	
														  	<%= f.number_field :meetingPercent, class: "form-control" %>
													    </div>
													  </td>		  		  
													  <td>
													  	<div class="form-group my_td">		    	
														  	<%= f.time_field :meetingDuration, class: "form-control"%>
													    </div>
													  </td>
											    </tr>
											    <tr>
											  		<td>Auxiliary</td>
											      <td>
													  	<div class="form-group my_td">		    	
														  	<%= f.number_field :auxPercent, class: "form-control" %>
													    </div>
													  </td>		  		  
													  <td>
													  	<div class="form-group my_td">		    	
														  	<%= f.time_field :auxDuration, class: "form-control"%>
													    </div>
													  </td>
											    </tr>
											    <tr>
											  		<td>Outdoor</td>
											      <td>
													  	<div class="form-group my_td">		    	
														  	<%= f.number_field :outPercent, class: "form-control" %>
													    </div>
													  </td>		  		  
													  <td>
													  	<div class="form-group my_td">		    	
														  	<%= f.time_field :outDuration, class: "form-control"%>
													    </div>
													  </td>
											    </tr>
											  </tbody>
											</table>
										</div>
									</div>
								</div>
							</div>
							<div class="row my_row my_right">
								<div class="col-lg-11 col-md-11 col-sm-11 col-xs-11"></div>
								<div class="col-lg-1 col-md-1 col-sm-1 col-xs-1">
									<%= f.submit "Update" , class: "btn btn-large btn-primary"%>
								</div>	
							</div>
			        
						</div>
			    </div>
		    </div>
		  </div>
	  <% end %>
	<% end %>
</div>
